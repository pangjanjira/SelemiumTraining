<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="3" name="BlogSuite" parallel="tests">

	<test name="FirefoxTest">
		<parameter name="browser" value="firefox" />
		<parameter name="version" value="109.0" />
		<parameter name="platform" value="WIN8" />
		<classes>
			<class name="testSeleniumLambda.Selenium101LambdaTest" />
		</classes>
	</test>

	<test name="ChromeTest">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="111.0" />
		<parameter name="platform" value="WIN10" />
		<classes>
			<class name="testSeleniumLambda.Selenium101LambdaTest" />
		</classes>
	</test>

	<test name="SafariTest">
		<parameter name="browser" value="Opera" />
		<parameter name="version" value="85.0" />
		<parameter name="platform" value="macOS High Sierra" />
		<classes>
			<class name="testSeleniumLambda.Selenium101LambdaTest" />
		</classes>
	</test>

</suite>